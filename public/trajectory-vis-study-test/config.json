          "prompt": "Gender",
          "options": [
            "Male",
            "Female",
            "Others",
            "Prefer not to say"
          ]
        },
        {
          "id": "age",
          "type": "radio",
          "required": true,
          "prompt": "Age",
          "options": [
            "25-",
            "25-35",
            "35-45",
            "45-55",
            "55-65",
            "65+",
            "Prefer not to say"
          ]
        },
        {
          "id": "experience_vis",
          "type": "radio",
          "required": true,
          "prompt": "Experience with visualizations",
          "options": [
            "None",
            "Heard of it",
            "Somewhat",
            "Used it a few times",
            "Used it many times",
            "Prefer not to say"
          ]
        },
        {
          "id": "experience_dim_reduction",
          "type": "radio",
          "required": true,
          "prompt": "Experience with dimensionality reduction",
          "options": [
            "None",
            "Heard of it",
            "Somewhat",
            "Used it a few times",
            "Used it many times",
            "Prefer not to say"
          ]
